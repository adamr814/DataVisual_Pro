                                    <TextBox x:Name="_ProviderBox" HorizontalAlignment="Left" Margin="10,0,0,10" TextWrapping="Wrap" Width="120" Height="20" VerticalAlignment="Bottom" FontFamily="Cascadia Mono Light" FontSize="14" Grid.Row="3"/>
