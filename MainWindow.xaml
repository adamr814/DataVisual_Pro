                                    <TextBlock HorizontalAlignment="Left" Margin="444,0,0,33" TextWrapping="Wrap" Text="Weight" VerticalAlignment="Bottom" Height="16" Width="60" FontFamily="Cascadia Mono Light" FontSize="14" Grid.Row="2"/>
