                                    <TextBox x:Name="_AllergiesBox" HorizontalAlignment="Left" Margin="10,0,0,10" TextWrapping="Wrap" Width="237" Height="20" VerticalAlignment="Bottom" FontFamily="Cascadia Mono Light" FontSize="14" Grid.Row="4"/>
